{
  "extends": ["rapid7/browser"],
  "globals": {
    "__dirname": true,
    "Buffer": true,
    "global": true,
    "module": true,
    "process": true,
    "require": true
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "indent": 0,
    "object-curly-newline": 0,
    "object-curly-spacing": 0,
    "prefer-rest-params": 0,
    "rapid7/named-import-newline": 0,
    "space-before-function-paren": 0
  }
}
